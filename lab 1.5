#include <iostream>
#include <math.h>
using namespace std;

void main()
{
		float a, b, n1, n2, n3, n4;
		cout << "Enter two digits" << endl;
		cin >> a >> b;
		a = abs(a);
		b = abs(b);
		n1 = a + b; n2 = a - b; n3 = a * b; n4 = a / b;
		cout << n1 << endl << n2 << endl << n3 << endl << n4 << endl;
}
